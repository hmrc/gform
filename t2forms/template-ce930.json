{
  "_id": "CE930",
  "formName": "Gas for use as road fuel",
  "description": "Fill in your gas for use as road fuel return form online",
  "dmsSubmission": {
    "customerId": "${auth.payenino}",
    "classificationType": "BT-NRU-Environmental",
    "businessArea": "FinanceOpsCorpT"
  },
  "authConfig":{
    "authModule": "legacyEEITTAuth",
    "regimeId": "GF"
  },
  "submitSuccessUrl": "http://www.google.co.uk",
  "submitErrorUrl": "http://www.yahoo.co.uk",
  "sections": [
    {
      "title": "Enter your business details",
      "shortName": "Your business details",
      "fields": [
        {
          "id": "businessName",
          "type": "text",
          "label": "Business name"
        },
        {
          "id": "businessPhone",
          "type": "text",
          "format": "telephoneNumber",
          "label": "Phone number"
        },
        {
          "id": "businessAddress",
          "type": "address",
          "label": "Registered business address",
          "international": "yes"
        }
      ]
    },
    {
      "title": "Do you have duty to pay in this return period?",
      "fields": [
        {
          "type": "choice",
          "label": "",
          "id": "dutydue",
          "choices": [
            "Yes, I have duty to declare",
            "No, I want to submit a nil return"
          ]
        }
      ]
    },
    {
      "includeIf": "${dutydue=0}",
      "title": "Enter how much duty you owe",
      "description": "Select the gas you need to pay duty on from the list below.",
      "fields": [
        {
          "id": "fuelType591",
          "type": "group",
          "label": "Tax type 591 - natural gas",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "taxType591Quantity",
              "type": "text",
              "format": "number(11, 2, 'kg')",
              "label": "Quantity of gas in kilograms (kg) sent out, set aside or used as road fuel",
              "mandatory": "false"
            },
            {
              "id": "taxType591DutyDue",
              "type": "text",
              "format": "sterling",
              "label": "Amount of excise duty due",
              "mandatory": "false"
            }
          ]
        },
        {
          "id": "fuelType592",
          "type": "group",
          "label": "Tax type 592 - other gas",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "taxType592Quantity",
              "type": "text",
              "format": "number(11, 2, 'kg')",
              "label": "Quantity of gas in kilograms (kg) sent out, set aside or used as road fuel",
              "mandatory": "false"
            },
            {
              "id": "taxType592DutyDue",
              "type": "text",
              "format": "sterling",
              "label": "Amount of excise duty due",
              "mandatory": "false"
            }
          ]
        },
        {
          "id": "taxCodeOtherGroup",
          "type": "group",
          "label": "Add other tax codes",
          "helpText": "If the tax code you want to pay duty for does not appear in the list above, enter it below. \n\n[Check your tax code](https://www.gov.uk/government/publications/uk-trade-tariff-excise-duties-reliefs-drawbacks-and-allowances/uk-trade-tariff-excise-duties-reliefs-drawbacks-and-allowances)",
          "repeatsMax": 20,
          "repeatAddAnotherText": "Add other tax code",
          "presentationHint": "summariseGroupAsGrid",
          "fields": [
            {
              "id": "taxCodeOtherGroupCode",
              "type": "text",
              "label": "Tax code"
            },
            {
              "id": "taxCodeOtherGroupQuantity",
              "type": "text",
              "label": "Quantity in kilograms",
              "format": "number(12,2,'kg')"
            },
            {
              "id": "taxCodeOtherGroupAmount",
              "type": "text",
              "label": "Amount",
              "format": "sterling"
            }
          ]
        },
        {
          "id": "totalDue",
          "type": "text",
          "format": "sterling",
          "label": "Total payable",
          "submitMode": "readonly",
          "presentationHint": "totalValue",
          "value": "${taxCodeOtherGroupAmount.sum + taxType592DutyDue + taxType591DutyDue}"
        }
      ]
    },
    {
      "title": "Enter your contact person's details",
      "shortName": "Contact person",
      "description": "This is the person we will contact if we need to discuss your return.",
      "fields": [
        {
          "id": "contactName",
          "type": "text",
          "label": "Full name"
        },
        {
          "id": "contactEmailAddress",
          "type": "text",
          "label": "Email address",
          "format": "email"
        }
      ]
    }
  ],
  "declarationSection": {
    "shortName": "Sign and submit your return",
    "title": "Sign and submit your return",
    "fields": [
      {
        "id": "declarationNameGroup",
        "type": "group",
        "label": "Approver's details",
        "helpText": "Tell us who is approving this return.",
        "fields": [
          {
            "id": "declarationFullName",
            "type": "text",
            "label": "Full name"
          },
          {
            "id": "declarationJobTitle",
            "type": "text",
            "label": "Job title",
            "helpText": "Example: Finance Director"
          }
        ]
      },
      {
        "id": "declarationGroup",
        "type": "group",
        "label": "Declaration",
        "fields": [
          {
            "id": "declarationText",
            "type": "info",
            "label": "",
            "infoType": "noformat",
            "infoText": "By submitting the return, you confirm that the details provided are true and complete.  \n\nIf any of these details change, you must tell us within 7 days."
          }
        ]
      }
    ]
  },
  "acknowledgementSection": {
    "shortName": "Acknowledgement Page",
    "title": "Acknowledgement Page",
    "fields": [
      {
        "id": "ackPageInfoTop",
        "type": "info",
        "infoType": "noformat",
        "label": "If you owe duty",
        "infoText": "## If you owe duty \n\nIf you have a Direct Debit set up for this tax, you don't need to do anything. Your payment will be collected automatically.  \n\nOtherwise, paying electronically is an easy and secure way to pay. Use the details below and make sure you include your registration number (XZGR 00000 123456) on any payment.  \n\nSort code: 08-32-00  \nAccount number: 12000903  \nAccount name: HMRC Miscellaneous"
      },
      {
        "id": "ackPageWarning",
        "type": "info",
        "infoType": "important",
        "label": "",
        "infoText": "Make sure you pay the duty owed or you may face a penalty."
      },
      {
        "id": "ackPageInfoBottom",
        "type": "info",
        "infoType": "noformat",
        "label": "If you need to contact us about your return",
        "infoText": "## If you need to contact us about your return \n\nCall [Content TBC] or email [TBC], quoting your registration number XZGR 00000 123456."
      }
    ]
  }
}
