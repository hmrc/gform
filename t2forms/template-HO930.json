{
  "_id": "HO930",
  "formCategory": "hmrcReturnForm",
  "formName": "Biofuels and other fuel substitutes",
  "description": "",
  "dmsSubmission": {
    "customerId": "${auth.payenino}",
    "classificationType": "BT-NRU-Environmental",
    "businessArea": "FinanceOpsCorpT"
  },
  "authConfig":{
    "authModule": "legacyEEITTAuth",
    "regimeId": "GF"
  },
  "submitSuccessUrl": "http://www.google.co.uk",
  "submitErrorUrl": "http://www.yahoo.co.uk",
  "sections": [
    {
      "title": "Enter your business details",
      "shortName": "Your business details",
      "fields": [
        {
          "id": "businessName",
          "type": "text",
          "label": "Business name"
        },
        {
          "id": "businessPhone",
          "type": "text",
          "format": "telephoneNumber",
          "label": "Phone number"
        },
        {
          "id": "businessAddress",
          "type": "address",
          "label": "Registered business address",
          "international": "yes"
        }
      ]
    },
    {
      "title": "Enter the start and end dates for this return",
      "shortName": "Your claim period",
      "fields": [
        {
          "id": "returnStartDate",
          "type": "date",
          "label": "Start date",
          "helpText": "Example: 30 09 2017",
          "errorMessage": "Please enter a valid start date"
        },
        {
          "id": "returnEndDate",
          "type": "date",
          "label": "End date",
          "helpText": "Example: 31 12 2017",
          "format": "after ${returnStartDate}",
          "errorMessage": "Please enter a valid end date.  Note this must be after the start date."
        }
      ]
    },
    {
      "title": "Do you have duty to pay in this return period?",
      "shortName": "Do you have duty to pay?",
      "fields": [
        {
          "id": "dutyToPayChoice",
          "type": "choice",
          "label": "",
          "choices": [
            "Yes, I have duty to declare",
            "No, I want to submit a nil return"
          ]
        }
      ]
    },
    {
      "title": "Enter how much duty you owe",
      "shortName": "Duty details",
      "description": "Select the biofuel, substitute or additive you need to pay duty on.",
      "includeIf": "${dutyToPayChoice=0}",
      "fields": [
        {
          "id": "taxType33589Group",
          "type": "group",
          "label": "Tax type 33589 - Biodiesel",
          "presentationHint": "summariseGroupAsGrid, collapseGroupUnderLabel",
          "fields": [
            {
              "id": "taxType33589Quantity",
              "type": "text",
              "format": "number(11, 2, 'litres')",
              "label": "Quantity in litres of biofuel, substitute or additive",
              "mandatory": "false"
            },
            {
              "id": "taxType33589DutyDue",
              "type": "text",
              "format": "sterling",
              "label": "Amount of excise duty due",
              "mandatory": "false"
            }
          ]
        },
        {
          "id": "taxType33571Group",
          "type": "group",
          "label": "Tax type 33571 - Biodiesel for non road use",
