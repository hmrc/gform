            {
              "id": "taxType33571Quantity",
              "type": "text",
              "format": "number(11, 2, 'litres')",
              "label": "Quantity in litres of biofuel, substitute or additive",
              "mandatory": "false"
            },
            {
              "id": "taxType33571DutyDue",
              "type": "text",
              "format": "sterling",
              "label": "Amount of excise duty due",
              "mandatory": "false"
            }
          ]
        },
        {
          "id": "taxType33595Group",
